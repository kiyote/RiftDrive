@inherits NavBarComponent

    <Bar Breakpoint="Breakpoint.Desktop" Background="Background.Light" Theme="Theme.Light">
        <BarBrand>
            RiftDrive
        </BarBrand>
        <BarToggler>
        </BarToggler>
        <BarMenu>
            <BarStart>
                <BarItem>
                    <BarLink To="#home">Home</BarLink>
                </BarItem>
                <BarItem>
                    <BarLink To="#docs">Documentation</BarLink>
                </BarItem>
                <BarItem>
                    <BarDropdown>
                        <BarDropdownToggle>Dropdown</BarDropdownToggle>
                        <BarDropdownMenu>
                            <BarDropdownItem>Action</BarDropdownItem>
                            <BarDropdownItem>Another action</BarDropdownItem>
                        </BarDropdownMenu>
                    </BarDropdown>
                </BarItem>
            </BarStart>
            <BarEnd>
                <BarItem>
                    <BarLink To=@SignUpUrl>Sign Up</BarLink>
                </BarItem>
                <BarItem>
                    <BarLink To=@LogInUrl>Log in</BarLink>
                </BarItem>
            </BarEnd>
        </BarMenu>
    </Bar>